<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <title>Persistence Laboratory</title>
    <% include partials/head %>
  </head>
  <body>

    <!-- HEADER -->
    <% include partials/navigation %>

    <!-- MAIN CONTENT -->

        <main>
            <div class="signin">
                <form action="#" class="sign-in-form">
                    <h2 class="title">Physician Information</h2>
                    <h2 class="info">Name</h2>
                    <div class="input-field" id="input-info">
                        <span id="user" class="material-icons-outlined">account_circle</span>
                        <input type="text" placeholder="Dr. Doe" required=""/>
                    </div>
                    <h2 class="info">E-mail</h2>
                    <div class="input-field" id="input-info">
                        <span id="user" class="material-icons-outlined">mail</span>
                        <input type="text" placeholder="dr.johndoe@medic.com" required=""/>
                    </div>
                    <h2 class="info">License ID</h2>
                    <div class="input-field" id="input-info">
                        <span id="user" class="material-icons-outlined">school</span>
                        <input type="text" placeholder="12345" required=""/>
                    </div>
                    <h2 class="info">Specialty</h2>
                    <div class="input-field" id="input-info">
                        <span id="user" class="material-icons">attribution</span>
                        <input type="text" placeholder="Specialty" required=""/>
                    </div>
                    <input type="submit" value="Add Physician" class="btn"/>
                </form>
            </div>
        </main>
    </body>

    <!-- section one -->

    <!-- FOOTER -->
    <% include partials/footer %>

